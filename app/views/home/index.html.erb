<% if @user.games == [] %>
  <h2>No Games! Get playing.</h2>
 <% else %>
  <h2><% @games.each do |game| %>
    <p><%= game.created_at.strftime("%m/%d/%Y") %></p>
    <% end %>
  </h2>
<% end %>

<%= button_to "Update Account", edit_user_registration_path, method: :get %>

<%= button_to "Log Out", destroy_user_session_path, method: :delete %>
